<div class="new yep">
    <%= link_to 'Add New +', new_venue_path %>
</div>
<% @venues.each do |venue| %>
    <div class="venues yep">
        <h2><%= venue.name %></h2>
        <span class="yep">Latitude: <%= venue.latitude %></span>
        <span class="yep">Longitude: <%= venue.longitude %></span>
        <span class="yep">Times here: <%= venue.footprints.count %></span>
    </div><!-- END OF VENUES -->
<% end %>